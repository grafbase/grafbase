    "lint": "eslint src/*.ts*"
  },
  "peerDependencies": {
    "next": "^12 || ^13 || ^14.0.0",
    "react": "*",
    "react-dom": "*",
    "grafbase": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.0.0",
    "eslint-config-grafbase": "workspace:*",
    "next": "^14.0.0",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "5.4.4"
  }
}
